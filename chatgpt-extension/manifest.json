{
  "manifest_version": 3,
  "name": "ChatGPT Web-to-API",
  "version": "1.0",
  "description": "Use ChatGPT web token as API in browser",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*.openai.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "ChatGPT API"
  }
}